<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vidhyalearning.myquizapp">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NewUser"
            android:label="@string/title_activity_new_user" />
        <activity
            android:name=".AdminActivity"
            android:label="@string/title_activity_admin" />
        <activity android:name=".OpenCategory" />
        <activity
            android:name=".adminquiz"
            android:label="@string/title_activity_adminquiz" />
        <activity android:name=".questionListDisplay" />
        <activity android:name=".editQuestion" />
        <activity android:name=".UserActivity" />
        <activity
            android:name=".userQuiz"
            android:configChanges="orientation|screenSize"></activity>
        <activity android:name=".XMLText" />
        <activity android:name=".reviewquiz"></activity>
    </application>

</manifest>